FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     simpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         2000;

deltaT          1;

writeControl    timeStep;

writeInterval   1000;

purgeWrite      1;

writeFormat     ascii;

writePrecision  16;

writeCompression true;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

functions
{
    solverInfo
    {
        type                solverInfo;
        libs                ("libutilityFunctionObjects.so");
        fields              (U p);
        writeResidualFields false;
    }
	pt
	{
		type         pressure;
		libs         ("libfieldFunctionObjects.so");
		writeControl writeTime; 
        //writeControl    timeStep;
        //writeInterval   500;
		log          false;
        mode         totalCoeff;
		pInf         0;
		UInf         ( 0.039473 0 0 );
        rho          rhoInf;
		rhoInf       1;
	}
	pstatic
	{
		type         pressure;
		libs         ("libfieldFunctionObjects.so");
		writeControl writeTime;
		log          false;
        mode         staticCoeff;
		pInf         0;
		UInf         ( 0.039473 0 0 );
        rho          rhoInf;
		rhoInf       1;
	}
    #includeFunc "ptInlet";
    #includeFunc "ptOutlet";
    #includeFunc "psUpper";
    #includeFunc "psLower";
    #includeFunc "flowRateOutlet";
}


// ************************************************************************* //
